{
	"name": "starter-svelte",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "concurrently \"npm:dev:*\" --restart-tries 1",
		"dev:svelte": "vite dev --host",
		"dev:storybook": "storybook dev -p 6006 --no-open",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"build": "vite build && npm run optimize-images",
		"optimize-images": "image-opti --source ./build/images --target ./build/images",
		"build:vercel": "vite build && npm run optimize-images:vercel",
		"optimize-images:vercel": "image-opti -s .vercel/output/static/images -t .vercel/output/static/images -v"
	},
	"devDependencies": {
		"@kesval/design": "^4.1.2",
		"@kesval/image": "^0.2.0",
		"@kesval/image-svelte": "^0.2.1",
		"@storybook/addon-a11y": "^7.4.5",
		"@storybook/addon-docs": "^7.4.5",
		"@storybook/addon-essentials": "^7.4.5",
		"@storybook/addon-interactions": "^7.4.5",
		"@storybook/addon-links": "^7.4.5",
		"@storybook/addon-storysource": "^7.4.5",
		"@storybook/addon-svelte-csf": "^4.0.9",
		"@storybook/blocks": "^7.4.5",
		"@storybook/svelte": "^7.4.5",
		"@storybook/sveltekit": "^7.4.5",
		"@storybook/testing-library": "^0.2.2",
		"@storybook/theming": "^7.4.5",
		"@sveltejs/adapter-vercel": "^3.0.3",
		"@sveltejs/kit": "^1.25.1",
		"@total-typescript/ts-reset": "^0.5.1",
		"@types/cookie": "^0.5.2",
		"@types/node": "^20.8.2",
		"@types/nprogress": "^0.2.1",
		"@typescript-eslint/eslint-plugin": "^6.7.4",
		"@typescript-eslint/parser": "^6.7.4",
		"autoprefixer": "^10.4.14",
		"bits-ui": "^0.5.7",
		"clsx": "^2.0.0",
		"concurrently": "^8.2.1",
		"cookie": "^0.5.0",
		"eslint": "^8.50.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-perfectionist": "^2.1.0",
		"eslint-plugin-storybook": "^0.6.14",
		"eslint-plugin-svelte": "^2.33.2",
		"lucide-svelte": "^0.284.0",
		"nprogress": "^0.2.0",
		"postcss": "^8.4.24",
		"postcss-load-config": "^4.0.1",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"sass": "^1.68.0",
		"storybook": "^7.4.5",
		"svelte": "^4.2.1",
		"svelte-check": "^3.5.2",
		"svelte-i18n": "^3.7.4",
		"sveltekit-view-transition": "^0.5.0",
		"tailwind-merge": "^1.14.0",
		"tailwind-variants": "^0.1.14",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.4.9",
		"zod": "^3.22.2"
	},
	"type": "module"
}
