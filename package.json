{
  "name": "kesval.com",
  "version": "0.0.1",
  "private": true,
  "author": {
    "url": "https://kesval.com",
    "email": "contact@kesval.com",
    "name": "Jordan 'KesvaL'"
  },
  "homepage": "https://kesval.com",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "postbuild": "npm run optimize-images",
    "optimize-images": "image-transmutation --run --sourceFolder './build/images' --targetFolder './build/images' --inputFormats 'jpg' --inputFormats 'jpeg' --inputFormats 'png' --outputFormats 'png' --outputFormats 'webp' --outputFormats 'avif'",
    "vercel-optimize-images": "image-transmutation --run --sourceFolder '.vercel/output/static/images' --targetFolder '.vercel/output/static/images' --inputFormats 'jpg' --inputFormats 'jpeg' --inputFormats 'png' --outputFormats 'png' --outputFormats 'webp' --outputFormats 'avif'",
    "vercel-build": "npm run build && npm run vercel-optimize-images"
  },
  "devDependencies": {
    "@kesval/design": "^4.1.2",
    "@sveltejs/kit": "^2.0.6",
    "@types/cookie": "^0.5.1",
    "@types/luxon": "^3.3.1",
    "@types/node": "^20.5.9",
    "@types/nprogress": "^0.2.0",
    "@types/prismjs": "^1.26.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vercel/analytics": "^1.0.2",
    "autoprefixer": "^10.4.15",
    "cookie": "^0.5.0",
    "cssnano": "^6.0.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.33.0",
    "postcss": "^8.4.28",
    "prettier": "^3.0.2",
    "prettier-plugin-svelte": "^3.0.3",
    "sass": "^1.66.1",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "unlighthouse": "^0.10.0",
    "vite": "^4.4.9",
    "web-vitals": "^3.4.0",
    "@sveltejs/adapter-vercel": "^3.0.2",
    "@types/js-search": "^1.4.0",
    "image-transmutation": "^0.1.2",
    "is-emoji-supported": "^0.0.5",
    "js-search": "^2.0.1",
    "luxon": "^3.4.2",
    "mdsvex": "^0.11.0",
    "nprogress": "^0.2.0",
    "prismjs": "^1.29.0",
    "reading-time": "^1.5.0",
    "rehype-accessible-emojis": "^0.3.2",
    "rehype-autolink-headings": "^7.0.0",
    "rehype-external-links": "^3.0.0",
    "rehype-slug": "^6.0.0",
    "striptags": "^3.2.0",
    "svelte-i18n": "^3.7.3"
  },
  "type": "module"
}

