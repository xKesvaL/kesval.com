{
  "name": "kesval.com",
  "version": "0.0.1",
  "private": true,
  "author": {
    "url": "https://kesval.com",
    "email": "contact@kesval.com",
    "name": "Jordan 'KesvaL'"
  },
  "homepage": "https://kesval.com",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "postbuild": "npm run optimize-images",
    "optimize-images": "image-transmutation --run --sourceFolder './build/images' --targetFolder './build/images' --inputFormats 'jpg' --inputFormats 'jpeg' --inputFormats 'png' --outputFormats 'png' --outputFormats 'webp' --outputFormats 'avif'",
    "vercel-optimize-images": "image-transmutation --run --sourceFolder '.vercel/output/static/images' --targetFolder '.vercel/output/static/images' --inputFormats 'jpg' --inputFormats 'jpeg' --inputFormats 'png' --outputFormats 'png' --outputFormats 'webp' --outputFormats 'avif'",
    "vercel-build": "npm run build && npm run vercel-optimize-images"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/adapter-static": "^2.0.1",
    "@sveltejs/kit": "^1.22.1",
    "@types/cookie": "^0.5.1",
    "@types/luxon": "^3.3.1",
    "@types/node": "^20.4.4",
    "@types/nprogress": "^0.2.0",
    "@types/prismjs": "^1.26.0",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^5.60.1",
    "@vercel/analytics": "^1.0.1",
    "autoprefixer": "^10.4.14",
    "cookie": "^0.5.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.27",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "sass": "^1.64.1",
    "svelte": "^4.1.1",
    "svelte-check": "^3.4.6",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.6",
    "web-vitals": "^3.3.1"
  },
  "type": "module",
  "dependencies": {
    "@kesval/design": "^1.8.1",
    "@sveltejs/adapter-vercel": "^3.0.0",
    "@types/js-search": "^1.4.0",
    "image-transmutation": "^0.1.2",
    "js-search": "^2.0.1",
    "luxon": "^3.3.0",
    "mdsvex": "^0.11.0",
    "nprogress": "^0.2.0",
    "prismjs": "^1.29.0",
    "reading-time": "^1.5.0",
    "rehype-accessible-emojis": "^0.3.2",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-external-links": "^2.1.0",
    "rehype-slug": "^5.1.0",
    "striptags": "^3.2.0",
    "xss": "^1.0.14"
  }
}
