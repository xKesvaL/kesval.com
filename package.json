{
	"name": "starter-svelte",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "concurrently \"npm:dev:*\" --restart-tries 1",
		"dev:svelte": "vite dev --host",
		"dev:storybook": "storybook dev -p 6006 --no-open",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"build": "paraglide-js compile --project ./project.inlang/settings.inlang && vite build"
	},
	"devDependencies": {
		"@inlang/paraglide-js": "1.0.0-prerelease.16",
		"@inlang/paraglide-js-adapter-vite": "^1.0.1",
		"@kesval/design": "^4.1.2",
		"@kesval/image": "^0.2.0",
		"@kesval/image-svelte": "^0.2.1",
		"@storybook/addon-a11y": "^7.6.3",
		"@storybook/addon-docs": "^7.6.3",
		"@storybook/addon-essentials": "^7.6.3",
		"@storybook/addon-interactions": "^7.6.3",
		"@storybook/addon-links": "^7.6.3",
		"@storybook/addon-storysource": "^7.6.3",
		"@storybook/addon-svelte-csf": "^4.0.13",
		"@storybook/blocks": "^7.6.3",
		"@storybook/svelte": "^7.6.3",
		"@storybook/sveltekit": "^7.6.3",
		"@storybook/testing-library": "^0.2.2",
		"@storybook/theming": "^7.6.3",
		"@sveltejs/adapter-vercel": "^3.1.0",
		"@sveltejs/enhanced-img": "^0.1.5",
		"@sveltejs/kit": "^1.27.6",
		"@total-typescript/ts-reset": "^0.5.1",
		"@types/js-cookie": "^3.0.6",
		"@types/node": "^20.10.3",
		"@types/nprogress": "^0.2.3",
		"@typescript-eslint/eslint-plugin": "^6.13.2",
		"@typescript-eslint/parser": "^6.13.2",
		"autoprefixer": "^10.4.16",
		"bits-ui": "^0.11.3",
		"clsx": "^2.0.0",
		"concurrently": "^8.2.2",
		"dayjs": "^1.11.10",
		"eslint": "^8.55.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-perfectionist": "^2.5.0",
		"eslint-plugin-storybook": "^0.6.15",
		"eslint-plugin-svelte": "^2.35.1",
		"is-emoji-supported": "^0.0.5",
		"js-cookie": "^3.0.5",
		"lucide-svelte": "^0.294.0",
		"nprogress": "^0.2.0",
		"postcss": "^8.4.32",
		"postcss-load-config": "^5.0.2",
		"prettier": "^3.1.0",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.9",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"sass": "^1.69.5",
		"storybook": "^7.6.3",
		"svelte": "^4.2.5",
		"svelte-check": "^3.6.2",
		"sveltekit-view-transition": "^0.5.2",
		"tailwind-merge": "^2.1.0",
		"tailwind-variants": "^0.1.18",
		"tailwindcss": "^3.3.6",
		"tslib": "^2.6.2",
		"typescript": "^5.3.2",
		"vite": "^5.0.0",
		"vite-plugin-kit-routes": "^0.2.4",
		"vite-plugin-watch": "^0.2.0",
		"zod": "^3.22.4"
	},
	"type": "module"
}
